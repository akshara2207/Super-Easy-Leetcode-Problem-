class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        triangle = []
        for i in range(numRows):
            # Start with 1 at the beginning of the row
            row = [1]
            # Add inner elements if row has more than 1 element
            if i > 0:
                for j in range(1, i):
                    row.append(triangle[i-1][j-1] + triangle[i-1][j])
                # End with 1
                row.append(1)
            triangle.append(row)
        return triangle
